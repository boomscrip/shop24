{
  "parser": "babel-eslint",
  "extends": ["airbnb", "eslint:recommended", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier", "react"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
